<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel width="100%" height="100%" ui:field="htmlPanel">
		<g:HorizontalPanel height="100%" width="100%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE" ui:field="topBarFrame" spacing="3">
			<g:Cell verticalAlignment="ALIGN_MIDDLE" width="6%" height="14%">
				<g:Button ui:field="aTerm" text="A"/>
			</g:Cell>
			<g:Cell verticalAlignment="ALIGN_MIDDLE" width="6%" height="14%">
				<g:Button ui:field="bTerm" text="B"/>
			</g:Cell>
			<g:Cell verticalAlignment="ALIGN_MIDDLE" width="6%" height="14%">
				<g:Button ui:field="cTerm" text="C"/>
			</g:Cell>
			<g:Cell verticalAlignment="ALIGN_MIDDLE" width="6%" height="14%">
				<g:Button ui:field="dTerm" text="D"/>
			</g:Cell>
			<g:Cell width="30%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE" height="100%">
				<g:HorizontalPanel width="100%" height="100%"/>
			</g:Cell>
					<g:Cell verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_RIGHT" height="14%" width="25%">
						<g:Button ui:field="scheduler" text="Scheduler" width="" height=""/>
					</g:Cell>
					<g:Cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE" width="10%" height="14%">
						<g:Button ui:field="inboxButton" text="Inbox" width="100%" height="14%"/>
					</g:Cell>
					<g:Cell verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_RIGHT" width="11%" height="14%">
						<g:Button ui:field="search" text="Search" width="100%" height="14%"/>
					</g:Cell>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 