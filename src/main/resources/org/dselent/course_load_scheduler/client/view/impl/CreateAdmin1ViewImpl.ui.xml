<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel width="450px" height="300px" horizontalAlignment="ALIGN_CENTER">
			<g:VerticalPanel width="300px" height="300px">
				<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
					<g:Label text="Select a Professor From the Panel on the Right" ui:field="instruction"/>
				</g:Cell>
			</g:VerticalPanel>
			<g:ListBox visibleItemCount="15" width="150px" height="300px" ui:field="professorList"/>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 