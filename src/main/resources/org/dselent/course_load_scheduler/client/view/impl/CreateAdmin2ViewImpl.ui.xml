<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel width="450px" height="300px" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
			<g:VerticalPanel height="300px" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE" width="200px">
				<g:Label text="First Name" ui:field="firstNameLabel"/>
				<g:Label text="Last Name" ui:field="lastNameLabel"/>
				<g:Label text="Username" ui:field="userNameLabel"/>
				<g:Label text="Professor Title" ui:field="titleLabel"/>
				<g:Label text="Department" ui:field="departmentLabel"/>
			</g:VerticalPanel>
			<g:VerticalPanel horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE" width="100px">
				<g:ToggleButton text="Make Admin" ui:field="adminToggle"/>
			</g:VerticalPanel>
			<g:ListBox visibleItemCount="15" width="150px" height="300px" ui:field="professorList"/>
		</g:HorizontalPanel>\
	</g:HTMLPanel>
</ui:UiBinder> 