
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel ui:field="createCourseHorizontalPanel">
			<g:VerticalPanel horizontalAlignment="ALIGN_CENTER" ui:field="createCourseVerticalPanel"
			verticalAlignment="ALIGN_MIDDLE" width="450px" height="299px">
			<g:Label ui:field="courseNameLabel" text="Course Name"></g:Label>
			<g:TextBox ui:field="courseNameTextBox" />
			<g:Label ui:field="courseNumberLabel" text ="Course Number" />
			<g:TextBox ui:field="courseNumberTextBox" />
			<g:Label ui:field="courseSectionsLabel" text="Course Section" />
			<g:TextBox ui:field="courseSectionsTextBox"/>
			<g:Button ui:field="createCourseButton" text="Create Course" width="50%" />
			
		</g:VerticalPanel>
			<g:ScrollPanel ui:field="courseScrollPanel" width="86px" height="299px"/>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 