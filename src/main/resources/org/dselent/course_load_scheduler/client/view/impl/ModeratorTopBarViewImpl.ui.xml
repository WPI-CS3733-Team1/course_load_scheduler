<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel width="100%" height="100%">
		<g:HorizontalPanel height="100%" width="100%" horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE" ui:field="topBarFrame" spacing="3">
			<g:Cell width="10%" height="100%">
				<g:HorizontalPanel width="33%" height="100%"/>
			</g:Cell>
			<g:Cell verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_RIGHT" width="28%" height="14%">
						<g:Button ui:field="createCourse" text="Create New Class" width="100%" height="14%"/>
					</g:Cell>
				<g:Cell verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_RIGHT" width="28%" height="14%">
						<g:Button ui:field="createSection" text="Create New Section" width="100%" height="14%"/>
					</g:Cell>
					
					<g:Cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE" width="28%" height="14%">
						<g:Button ui:field="createAdmin" text="Create New Admin" width="100%" height="14%"/>
					</g:Cell>
					<g:Cell verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_RIGHT" width="20%" height="14%">
						<g:Button ui:field="search" text="Search" width="100%" height="14%"/>
					</g:Cell>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 