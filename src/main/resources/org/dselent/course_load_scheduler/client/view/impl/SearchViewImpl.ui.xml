<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel width="100%" height="100%">
		<g:AbsolutePanel ui:field = "search_panel" width="100" height="558px">
			<g:at top="0" left="0">
				<g:VerticalPanel width="100%" height="25%">
					<g:Cell verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_CENTER">
						<g:RadioButton ui:field = "users_button" name="users_button" text="Users"/>
					</g:Cell>
					<g:Cell horizontalAlignment="ALIGN_CENTER">
						<g:RadioButton ui:field = "courses_button" name="courses_button" text="Courses"/>
					</g:Cell>
					<g:Cell horizontalAlignment="ALIGN_CENTER">
						<g:TextBox ui:field = "dept_box" text="Insert Department ID" textAlignment="ALIGN_CENTER"/>
					</g:Cell>
				</g:VerticalPanel>
			</g:at>
			<g:at top="140" left="0">
				<g:Grid width="100%" height="75%"/>
			</g:at>
		</g:AbsolutePanel>
	</g:HTMLPanel>
</ui:UiBinder> 